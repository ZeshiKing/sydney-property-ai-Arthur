{
  "name": "rental-aggregator-backend",
  "version": "1.0.0",
  "description": "Australian rental property aggregation backend service with real-time scraping and AI report generation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker build -t rental-aggregator .",
    "docker:tools": "docker-compose --profile tools up -d",
    "db:migrate": "echo 'Database migration placeholder'",
    "db:seed": "echo 'Database seeding placeholder'",
    "docs:generate": "echo 'API documentation generation placeholder'",
    "clean": "rm -rf dist coverage .nyc_output",
    "postinstall": "echo 'Setup complete! Run npm run dev to start development server'"
  },
  "keywords": [
    "rental",
    "property",
    "aggregator",
    "australia",
    "domain",
    "real-estate",
    "firecrawl",
    "typescript",
    "api",
    "scraping",
    "express",
    "postgresql",
    "redis",
    "bull-queue"
  ],
  "author": "Property Aggregator Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/rental-aggregator-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/rental-aggregator-backend/issues"
  },
  "homepage": "https://github.com/your-username/rental-aggregator-backend#readme",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "bull": "^4.12.0",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "cheerio": "^1.0.0-rc.12",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/pg": "^8.10.7",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/jest": "^29.5.6",
    "@types/cheerio": "^0.22.35",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.2.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}